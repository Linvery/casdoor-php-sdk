{
    "name": "casdoor/casdoor-php-sdk",
    "description": "PHP client SDK for Casdoor",
    "authors": [
        {
            "name": "basakest",
            "email": "ab1652759879@gmail.com"
        }
    ],
    "license": "Apache-2.0",
    "keywords": [
        "casdoor",
        "oauth",
        "saml",
        "authentication",
        "auth",
        "sso",
        "oidc",
        "authn"
    ],
    "require": {
        "php": ">=7.1.0",
        "guzzlehttp/guzzle": "^7.3",
        "league/oauth2-client": "^2.6",
        "phpunit/phpunit": "~7.0|~8.0|~9.0",
        "php-coveralls/php-coveralls": "^2.1",
        "phpstan/phpstan": "^1.2",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Casdoor\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0|~8.0|~9.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Casdoor\\Tests\\": "tests/"
        }
    },
    "version": "1.0.0",
    "scripts": {
        "test": "./vendor/bin/phpunit -v --testsuite test",
        "benchmark": "./vendor/bin/phpunit -v --no-coverage --testsuite benchmark",
        "analyse": "./vendor/bin/phpstan analyse"
    }
}